<!DOCTYPE html>
<html>
<head>
<style>
body{

  font-family: Verdana;
}
</style></head>
<body>



<script>
var color_td;
document.write("<table border='1px'>");

for(var i = 1; i < 11; i++) {

	document.write("<tr style='height:30px;'>");

	for(var j = 1; j < 11; j++) {

		if(j == 1 || i == 1 || i==10 || j==10) {
			color_td = "#f00";
		}
		else {
			if(j == 2 || i == 2|| i==9 || j==9) {
			color_td = "#0f0";
		}
		else{	if(j == 3 || i == 3|| i==8 || j==8) {
			color_td = "#00f";
		 } else{	if(j == 4 || i == 4) {
			color_td = "#0ff";
		} }}}


		document.write("<td style='width:30px;background-color:" + color_td + "'>" + i*j + "</td>");
	}
	document.write("</tr>");
}

document.write("</table>");
</script>

<p id="demo"></p>

</body>
</html>
